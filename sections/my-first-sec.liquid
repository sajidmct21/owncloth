

<section class="{{ section.class }}">
  <h2>{{ section.settings.title }}</h2>
</section>

{% schema %}
{
  "name": "My First Section",
  "tag": "section",
  "class": "slideshow",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Slideshow"
    }
  ],
  "presets": [
    {
      "name": "My First Section",
      "settings": {
        "title": "First Section"
      }
    }
  ]
}
{% endschema %}
